@{
ViewBag.Title = "Home Page";
}

<div class="row">
    @foreach (var item in ViewBag.items)
    {
    <div class="card">
        <img class="card-img-top" src="@item.image" alt="Card image">
        <div class="card-body">
            <div class="card-title">@item.Name</div>
            <div class="card-text">
                <p>@item.Description</p>
                <p>@item.Quantity</p>
                <p>@item.Price</p>
            </div>
        </div>
    </div>
    }
</div>